    "collections": "^5.1.5",
    "crc-32": "^1.2.0",
    "kafka-node": "^3.0.1",
    "lodash": "^4.17.11",
    "node-config-module": "git+https://github.com/bitmain-israel/node-config-module",
    "optimist": "^0.6.1",
    "osprey": "^0.5.0",
    "pusher-js": "^4.3.0",
    "request": "^2.88.0",
